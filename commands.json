{
    "title": "Launch Apps this",
    "rules":
    [
        {
        "description": "Launch apps by right shift+letters AM only.",
        "manipulators":
            [
                {
                  "type": "basic",
                  "from": {
                    "key_code": "c",
                    "modifiers": {
                      "mandatory": [
                        "left_control",
                        "left_alt",
                        "left_gui"
                      ],
                      "optional": [
                        "caps_lock"
                      ]
                    }
                  },
                  "to": [
                    {
                      "shell_command": "open -a 'Google Chrome.app'"
                    }
                  ]
                },
                {
                  "type": "basic",
                  "from": {
                    "key_code": "t",
                    "modifiers": {
                      "mandatory": [
                        "left_control",
